<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>


  <servlet>
    <servlet-name>LoginEmployeeServlet</servlet-name>
    <servlet-class>LoginEmployeeServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>LogoutServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>LoginEmployeeServlet</servlet-name>
    <url-pattern>/LoginEmployeeServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ManagerServlet</servlet-name>
    <servlet-class>ManagerServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>ManagerServlet</servlet-name>
    <url-pattern>/ManagerServlet</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>EmpProfileServlet</servlet-name>
    <servlet-class>EmpProfileServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>EmpProfileServlet</servlet-name>
    <url-pattern>/EmpProfileServlet</url-pattern>
  </servlet-mapping>

</web-app>
